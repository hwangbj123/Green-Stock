<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.green.greenstock.repository.interfaces.KospiCodeRepository"> 


	<select id="findMkscShrnIscdByHtsKorIsName" resultType="com.green.greenstock.repository.model.KospiCode">
		select mksc_shrn_iscd  
		from kospi_code kc  
		where hts_kor_isnm  = #{companyName};
	</select>

</mapper>